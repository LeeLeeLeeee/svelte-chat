{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$components/*": ["src/components/*"],
			"$stores/*": ["src/stores/*"],
			"$animations/*": ["src/animations/*"]
		},
		"types": ["cypress", "@testing-library/cypress"],
		"module": "ESNext",
		"target": "ESNext",
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"],
}
